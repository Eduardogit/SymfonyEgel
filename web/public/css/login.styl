html,body
	height: 100%
	width: 100%
#login
	margin-top 80px
	button
		border-radius 20px
		border 0px 
	form
		margin-bottom 70px
	hr
		border-bottom: 1px solid #222fc3!important;
		
input,select
	height: 45px!important
	border 0px!important
	background: #f1f1f1!important
	opacity: 0.7!important
	box-shadow: 0!important
	border-radius 20px!important
	color #337ab7!important
	font-weight: bold
li,a
	transition all .2s ease-out
	background-color: transparent!important
	border 0px
	margin-bottom 30px
li:active,li:focus,li:hover
	background-color: transparent!important
	color #fff!important
a:hover
	background-color: transparent!important
	border 0px
	border-bottom 2px solid #337ab7
	color #fff!important
.reg
	border-radius 20px
	border 0px
	margin-top 23px
.nav-tabs,.nav
	border-bottom 0px
	margin-left 30%
	text-align: center
